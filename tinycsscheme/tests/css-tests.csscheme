/* `*` is a special selector that expects a body with the general settings (like
 * background and foreground color, indent guides ...).
 *
 * All other @rules are arbitrary as long as only an identifier or a string
 * follows, e.g. name and uuid or "foldingStartMatches" or whatever, if you need
 * that. They will be added as an arbitrary dict entry with a string value.
 */
@name "Test Scheme";
/* a string */
@uuid 69871a78-3515-43a2-bbc0-81bdc8c3d745;
@uuId 69871a78-3515-43a2-bbc0-81bdc8c3d74;

* {
  background: #111111;
  foreground: #888;
  unknown: hsl(0,0,0);
  lineHighlight: #12345678; }

source {
  foreground: #555555;
  fontStyle: bold italic;
  @random-at-rule yeah; }

text {
  /* optional name in at-rule */
  @name "Text";
  foreground: green;
  fontStyle: bold italic; }

* string {
  foreground: rgba(-20%, /**/2, 2.1, .1);
  fontStyle: italic; }

  string constant {
    foreground: #12345678; }

/* invalid usage of # */
String and Number#string, /**/ number - /**/number.constant {
  @scope sure;
  foreground: #0000FF;
  fontStyle: underline; }
